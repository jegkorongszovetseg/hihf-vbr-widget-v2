{
  "compilerOptions": {
    "target": "esnext",
    "module": "ES6",
    "baseUrl": "./",
    "paths": {
      "@shared/*": ["src/@shared/*"],
      "vue": ["node_modules/vue/*"],
      "@vueuse/core": ["node_modules/@vueuse/core/*"]
    }
  },
  "include": ["node_modules/vue/**/*", "node_modules/@vueuse/**/*"],
}
