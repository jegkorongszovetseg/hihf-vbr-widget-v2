{
  "compilerOptions": {
    "target": "esnext",
    "module": "ES6",
    "baseUrl": "./",
    "paths": {
      "@VbrWidget/core": ["src/@core/index.js"],
      "@VbrWidget/components": ["src/@core/components/index.js"],
      "@VbrWidget/composables": ["src/@core/composables/index.js"],
      "@VbrWidget/utils": ["src/@core/utils/index.js"],
      "vue": ["node_modules/vue/*"],
      "@vueuse/core": ["node_modules/@vueuse/core/*"]
    }
  },
  "include": ["node_modules/vue/**/*", "node_modules/@vueuse/**/*", "src/**/*"],
}
