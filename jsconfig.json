{
  "compilerOptions": {
    "target": "esnext",
    "module": "ES6",
    "baseUrl": "./",
    "paths": {
      "@VbrWidget/core/*": ["src/@core/*"],
      "@VbrWidget/core/components/*": ["src/@core/components/*"],
      "@VbrWidget/core/composables/*": ["src/@core/composables/*"],
      "@VbrWidget/core/utils/*": ["src/@core/utils/*"],
      "vue": ["node_modules/vue/*"],
      "@vueuse/core": ["node_modules/@vueuse/core/*"]
    }
  },
  "include": ["node_modules/vue/**/*", "node_modules/@vueuse/**/*", "src/**/*"],
}
