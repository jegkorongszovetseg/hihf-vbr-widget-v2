{
  "name": "mjsz-vbr-elements",
  "version": "2.1.0-beta.0",
  "description": "Data visualization for MJSZ VBR",
  "packageManager": "pnpm@8.6.12",
  "workspaces": [
    "packages/*",
    "playground/**"
  ],
  "files": [
    "build"
  ],
  "scripts": {
    "playground": "pnpm run -C playground dev",
    "build": "rimraf build && pnpm -r --filter='./packages/*' run build",
    "release": "bumpp -r",
    "publish:release": "pnpm run build && pnpm publish --access public -r --filter=@mjsz-vbr-elements/*"
  },
  "keywords": [],
  "author": "√Åkos Stegner <akos.stegner@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "bumpp": "^9.2.0",
    "rimraf": "^5.0.1"
  }
}
